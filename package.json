    "sass-loader": "^7.1.0",
    "sinon": "^7.2.3",
    "sinon-chai": "^3.3.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.1",
    "webpack": "4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
